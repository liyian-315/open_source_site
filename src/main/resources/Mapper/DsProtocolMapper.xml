<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdu.open.source.site.repository.DsProtocolDao">
    <select id="findByIds" resultType="com.sdu.open.source.site.entity.DsProtocol">
        SELECT *
        FROM ds_protocol
        WHERE id IN
        <foreach collection="dsProtocolIds" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
</mapper>
